{
  "short_name": "React App",
  "name": "Words",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
      "debugger",
      "activeTab",
      "tabs",
      "background",
      "https://*/",
      "http://*/"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "static/js/0.chunk.js",
        "static/js/3.chunk.js",
        "static/js/4.chunk.js",
        "mark.min.js",
        "/static/js/scanner.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts":["background.js"]
  }
}

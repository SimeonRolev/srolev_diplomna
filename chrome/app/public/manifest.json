{
  "short_name": "React App",
  "name": "Words",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
      "debugger",
      "activeTab",
      "tabs",
      "background",
      "https://*/",
      "http://*/"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts":["background.js"]
  }
}
